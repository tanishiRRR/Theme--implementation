<h2>ジャンル編集</h2>
<%= form_with model: @genre, url: admin_genre_path(@genre.id), method: :patch do |f| %>
  <table>
    <tbody>
      <tr>
        <td>ジャンル名</td>
        <td><%= f.text_field :name, autofocus: true, placeholder:"ジャンル名" %></td>
        <td><%= f.submit "変更を保存" %></td>
        </tr>
    </tbody>
  </table>
<% end %>